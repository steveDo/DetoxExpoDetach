{
  "name": "ferriswheel-mobile",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-plugin-import-graphql": "^2.6.2",
    "babel-preset-expo": "^5.1.1",
    "bunyan": "^1.8.12",
    "copyfiles": "^2.0.0",
    "detox": "^13.0.0",
    "detox-expo-helpers": "^0.6.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.2.1",
    "expo-cli": "^2.21.1",
    "expo-detox-hook": "^1.0.10",
    "jest": "^24.8.0",
    "jest-expo": "^32.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2",
    "react-native-scripts": "1.14.0",
    "react-native-testing-library": "^1.7.0",
    "react-test-renderer": "16.3.1",
    "recyclerlistview": "https://tristan-z8l@bitbucket.org/ferriswheeldotai/recyclerlistview.git"
  },
  "main": "./app/index.js",
  "scripts": {
    "start": "rm -rf ./node_modules/.cache/babel-loader && react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest",
    "start-expo": "expo start",
    "run-ios": "react-native run-ios",
    "iosx": "react-native run-ios --simulator=\"iPhone X\"",
    "dev": "npm-run-all --parallel start-expo iosx",
    "format": "prettier --print-width 100 --single-quote --trailing-comma es5 --semi --write app/**/*.js",
    "test:e2e": "detox test -c ios.sim.debug e2e/app.test.js",
    "test:e2e:build": "detox build --configuration ios.sim.debug",
    "postinstall": "sed -i '' 's/startsWith/includes/g' node_modules/react-native/local-cli/runIOS/findMatchingSimulator.js; cd node_modules/react-native; exec ./scripts/ios-install-third-party.sh; cd third-party/glog-0.3.4; exec ./configure; cd ../../../..; rndebugger-open"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/"
    ],
    "moduleNameMapper": {
      "styled-components": "<rootDir>/node_modules/styled-components/native/dist/styled-components.native.cjs.js",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/mocks/fileMock.js"
    }
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/detoxexpo.app",
        "build": "xcodebuild -project ios/detoxexpo.xcodeproj -UseModernBuildSystem=NO -scheme detoxexpo -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "apollo-cache-inmemory": "^1.2.8",
    "apollo-client": "^2.4.0",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.2",
    "babel-plugin-import-graphql": "^2.6.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "card-validator": "^6.0.0",
    "downshift": "^3.1.0",
    "expo": "^32.0.0",
    "expo-errors": "^3.0.0",
    "expo-print": "^3.0.0",
    "expokit": "^1.7.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "lodash": "^4.17.10",
    "moment": "^2.23.0",
    "prop-types": "^15.6.2",
    "protobufjs": "^6.8.8",
    "react": "16.5.0",
    "react-apollo": "^2.5.5",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-animatable": "^1.3.2",
    "react-native-auth0": "^1.4.1",
    "react-native-awesome-card-io": "^0.8.2",
    "react-native-elements": "^0.19.1",
    "react-native-fast-image": "^5.1.1",
    "react-native-floating-labels": "^1.1.9",
    "react-native-keyboard-aware-scroll-view": "^0.7.4",
    "react-native-keyboard-manager": "github:ShikharQuovantis/react-native-keyboard-manager",
    "react-native-masked-text": "^1.9.2",
    "react-native-modal": "^7.0.1",
    "react-native-multi-slider": "https://github.com/Ferriswheeldotai/react-native-multi-slider/tarball/master",
    "react-native-progress": "^3.5.0",
    "react-native-render-html": "^4.1.2",
    "react-native-search-bar": "^3.4.2",
    "react-native-segmented-control-tab": "^3.3.1",
    "react-native-sentry": "^0.42.0",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-step-indicator": "0.0.9",
    "react-native-swipeable": "^0.6.0",
    "react-native-vector-icons": "^5.0.0",
    "react-native-walkthrough-tooltip": "^0.5.3",
    "react-navigation": "^3.0.9",
    "react-navigation-transitions": "^1.0.5",
    "sentry-expo": "^1.12.0",
    "smartlook-react-native-wrapper": "https://tristan-z8l@bitbucket.org/ferriswheeldotai/smartlook-react-native-wrapper.git",
    "styled-components": "^4.2.0"
  }
}
